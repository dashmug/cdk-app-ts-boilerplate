{
  "name": "cdk-app-ts-boilerplate",
  "version": "1.0.0",
  "description": "Opinionated CDK Typescript Boilerplate",
  "main": "bin/deploy.ts",
  "repository": "https://github.com/dashmug/cdk-app-ts-boilerplate",
  "author": "Noel Llevares <dashmug@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier . --ignore-unknown --write",
    "lint": "eslint . --cache",
    "typecheck": "tsc",
    "test": "jest",
    "synth": "cdk synth --strict --validation",
    "diff": "cdk diff",
    "deploy": "cdk deploy --strict --all --require-approval never",
    "destroy": "cdk destroy",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/aws-lambda": "8.10.138",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-n": "17.8.1",
    "eslint-plugin-promise": "6.2.0",
    "eslint-plugin-security": "3.0.0",
    "eslint-plugin-sonarjs": "1.0.3",
    "eslint-plugin-unicorn": "54.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "ts-jest": "29.1.4"
  },
  "dependencies": {
    "@tsconfig/node20": "20.1.4",
    "aws-cdk": "2.145.0",
    "aws-cdk-lib": "2.145.0",
    "cdk-nag": "2.28.142",
    "constructs": "10.3.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  }
}
