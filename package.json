{
  "name": "cdk-app-ts-boilerplate",
  "version": "1.0.0",
  "description": "Opinionated CDK Typescript Boilerplate",
  "main": "bin/deploy.ts",
  "repository": "https://github.com/dashmug/cdk-app-ts-boilerplate",
  "author": "Noel Llevares <dashmug@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier . --ignore-unknown --write",
    "lint": "eslint . --format=pretty --cache",
    "typecheck": "tsc",
    "test": "jest",
    "synth": "cdk synth --strict --validation",
    "diff": "cdk diff",
    "deploy": "cdk deploy --strict --all --require-approval never",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.3",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.44.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-formatter-pretty": "5.0.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "ts-jest": "29.1.1"
  },
  "dependencies": {
    "@tsconfig/node18-strictest": "1.0.0",
    "aws-cdk": "2.87.0",
    "aws-cdk-lib": "2.86.0",
    "cdk-nag": "2.27.57",
    "constructs": "10.2.66",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
