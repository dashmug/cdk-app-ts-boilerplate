{
  "name": "cdk-app-ts-boilerplate",
  "version": "1.0.0",
  "description": "Opinionated CDK Typescript Boilerplate",
  "main": "bin/deploy.ts",
  "repository": "https://github.com/dashmug/cdk-app-ts-boilerplate",
  "author": "Noel Llevares <dashmug@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier . --ignore-unknown --write",
    "lint": "eslint . --cache",
    "typecheck": "tsc",
    "test": "jest",
    "synth": "cdk synth --strict --validation",
    "diff": "cdk diff",
    "deploy": "cdk deploy --strict --all --require-approval never",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@types/aws-lambda": "8.10.130",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-n": "16.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "50.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1"
  },
  "dependencies": {
    "@tsconfig/node20": "20.1.2",
    "aws-cdk": "2.117.0",
    "aws-cdk-lib": "2.117.0",
    "cdk-nag": "2.27.227",
    "constructs": "10.3.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
