{
  "name": "cdk-app-ts-boilerplate",
  "version": "1.0.0",
  "description": "Opinionated CDK Typescript Boilerplate",
  "main": "bin/deploy.ts",
  "repository": "https://github.com/dashmug/cdk-app-ts-boilerplate",
  "author": "Noel Llevares <dashmug@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier . --ignore-unknown --write",
    "lint": "eslint . --cache",
    "typecheck": "tsc",
    "test": "jest",
    "synth": "cdk synth --strict --validation",
    "diff": "cdk diff",
    "deploy": "cdk deploy --strict --all --require-approval never",
    "destroy": "cdk destroy",
    "prepare": "if command -v husky; then husky install; fi"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@types/aws-lambda": "8.10.133",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.8.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "2.1.1",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2"
  },
  "dependencies": {
    "@tsconfig/node20": "20.1.2",
    "aws-cdk": "2.128.0",
    "aws-cdk-lib": "2.128.0",
    "cdk-nag": "2.28.35",
    "constructs": "10.3.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
